{
  "name": "create-mvp-app",
  "version": "1.1.2",
  "description": "CLI tool to scaffold production-ready Next.js MVPs with authentication, database, and UI components",
  "main": "dist/index.js",
  "bin": {
    "create-mvp-app": "dist/index.js"
  },
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest tests/generators tests/installers tests/utils tests/git",
    "test:integration": "vitest tests/integration",
    "verify": "pnpm build && pnpm test",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "nextjs",
    "mvp",
    "scaffolding",
    "cli",
    "boilerplate",
    "starter",
    "create-app"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "mustache": "^4.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.10.6",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
